<script id="slider" type="text/html">

  <li class="layui-nav-item layui-nav-itemed">
    <a href="javascript:;"><img src="{{d.bpic}}" width="40" height="40" style="vertical-align:middle;"><span style="padding:0 10px;line-height: 45px;display: inline-block;height: 40px;vertical-align: top;">{{d.type}}</span></a>
    <dl class="layui-nav-child">
    {{#  layui.each(d.zi,function(index, item){ }}
        {{# if(d.selectedindex=='0'&&index=='0'){ }}
        <dd class="layui-this">
          <a href="#" data-id="{{item.id}}"><span style="padding:0 10px">{{item.type}}</span></a>
        </dd>
        {{# }else{ }}
          <dd>
          <a href="#" data-id="{{item.id}}"><span style="padding:0 10px">{{item.type}}</span></a>
        </dd>
        {{# } }}

    {{#  }); }}
      
    </dl>
  </li>
</script>
<script id="goodsContent" type="text/html">

  <ul class="wd-list">
    {{# layui.each(d.goodinfo,function(index,item){ }}
      <li>
        
        <div class="inner">
          <div class="pic-wrap">
            <div class="img-vertical">
              <a href="#" class="img-wrap">
                <img width="230" height="230" src="{{item.goodpic}}" alt="">
              </a>
            </div>
          </div>
          <div class="txt red">
            
          </div>
          <div class="box">
            <div class="mutiCode">
              <dl>
                <dt>规格</dt>
                <dd>
                  {{# layui.each(item.pro,function(i,it){ }}
                    <span>{{it}}</span>
                  {{# }) }}
                </dd>
              </dl>
              <div class="layui-btn saveIntoMart" data-info="{{item.pro}}" data-id="{{item.goodid}}">确定</div>
              <input type="hidden" name="goods[]" value="">
            </div>
            <div class="info">
              <h3 class="title">
                <a href="#">{{item.goodname}}</a>
              </h3>
              <div class="price">
                抢新价
                <em>￥</em>
                {{# if(d.status=='0'){ }}
                  <span><input type="text" class="editPrice" data-id="{{item.storeid}}" style="width:70px;text-align:center;" placeholder="{{item.shopprice}}" value=""></span>
                {{# }else{ }}
                  <span>{{item.shopprice}}</span>
                {{# } }}
              </div>
            </div>
            <div class="shop-mask {{d.status}}" >
            {{# if(d.status=='0'){ }}
              <div class="addOrder" data-status="1" data-id="{{item.storeid}}">上架</div>
            {{# }else{ }}
              <div class="addOrder" data-status="0" data-id="{{item.storeid}}">下架</div>
            {{# } }}
              <h3 class="shop-name">
                <a href="#">万店联盟凤台路店</a>
              </h3>
            </div>
          </div>
        </div>
      </li>
    {{# }) }}
    
    
  </ul>
</script>

